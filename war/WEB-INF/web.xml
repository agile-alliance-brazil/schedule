<?xml version="1.0" encoding="UTF-8"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <listener>
    <listener-class>schedule.listener</listener-class>
  </listener>
  <servlet>
    <servlet-name>schedule.web/handler servlet</servlet-name>
    <servlet-class>schedule.servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>schedule.web/handler servlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
